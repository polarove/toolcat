{
  "name": "@polaris_liu/toolcat",
  "description": "A most usual tool package",
  "keywords": [
    "utils",
    "verifications"
  ],
  "type": "module",
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "sync": "yarn push:gitee && yarn push:github",
    "push:gitee": "git push -u gitee main",
    "push:github": "git push -u github main",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "version": "1.0.0",
  "author": "liuqi",
  "license": "MIT",
  "main": "index.ts",
  "dependencies": {
    "@types/node": "latest",
    "@vue/shared": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@esbuild-kit/cjs-loader": "latest",
    "commitizen": "latest",
    "cz-conventional-changelog": "latest",
    "fast-glob": "latest",
    "husky": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "latest"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
