{
  "name": "@polaris_liu/toolcat",
  "description": "A usual tool package",
  "keywords": [
    "utils",
    "verifications"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polarove/toolcat.git"
  },
  "version": "1.0.22",
  "author": "liuqi <liuqi6602@163.com>",
  "license": "MIT",
  "devDependencies": {
    "@cz-translated-changelog/zh-cn": "latest",
    "@translated-lint/zh-cn": "latest",
    "@types/node": "latest",
    "commitizen": "latest",
    "husky": "latest",
    "lint-staged": "^15.2.2",
    "typescript": "latest",
    "unbuild": "^2.0.0",
    "vitest": "latest"
  },
  "peerDependencies": {
    "@vue/shared": ">=3.0.0",
    "typescript": ">=5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@cz-translated-changelog/zh-cn"
    }
  }
}
